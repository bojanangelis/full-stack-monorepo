{
  "name": "@full-stack/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve:all": "npx concurrently --kill-others-on-fail \"yarn:start:*\"",
    "------START------": "------------",
    "start:verify-env": "yarn run validate:web-env && yarn run validate:api-env",
    "start:db:watch": "yarn run db:docker && yarn run db:migrate:dev && yarn run db:seed:dev && prisma generate --watch",
    "start:gql:watch": "graphql-codegen --config codegen.yml --watch",
    "start:api": "nx serve api",
    "start:web": "nx serve web",
    "------NX SECTION------": "------------",
    "build": "nx build",
    "test": "nx test",
    "------DB SECTION------": "------------",
    "db:docker": "npx env-cmd -f .env docker-compose -f tools/docker/dev.docker-compose.yml up -d --no-recreate --remove-orphans",
    "db:migrate:dev": "npx env-cmd -f .env npx prisma migrate dev",
    "db:studio": "npx env-cmd -f .env npx prisma studio",
    "db:seed:clear": "npx env-cmd -f .env ts-node --project tools/tsconfig.tools.json tools/database/seed-clear-database.ts",
    "db:seed:dev": "npx env-cmd -f .env ts-node --project tools/tsconfig.tools.json tools/database/seed.dev.ts",
    "------VALIDATE ENV SECTION------": "------------",
    "verify": "nx show projects && nx format:write && nx format:check && nx affected --target=lint --fix --parallel=3 && npx nx affected --target=test --parallel=3 && nx affected --target=build --parallel=3 --exclude=web",
    "validate:web-env": "npx env-cmd -f .env ts-node --project tools/tsconfig.tools.json tools/environment/validate-web-env.ts",
    "validate:api-env": "npx env-cmd -f .env ts-node --project tools/tsconfig.tools.json tools/environment/validate-api-env.ts"
  },
  "prisma": {
    "schema": "libs/api/data-access-db/src/lib/prisma/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@mercuriusjs/gateway": "^2.1.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/graphql": "^12.0.10",
    "@nestjs/mercurius": "^12.0.10",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/platform-fastify": "^10.2.8",
    "@next/font": "^14.0.2",
    "@prisma/client": "^5.5.2",
    "@swc/helpers": "~0.5.2",
    "@types/bcrypt": "^5.0.2",
    "axios": "^1.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "mercurius": "^13.2.2",
    "next": "13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/cypress": "17.1.1",
    "@nx/eslint": "17.0.3",
    "@nx/eslint-plugin": "^17.1.1",
    "@nx/jest": "17.0.3",
    "@nx/js": "17.1.1",
    "@nx/nest": "17.0.3",
    "@nx/next": "^17.1.1",
    "@nx/node": "17.0.3",
    "@nx/react": "17.1.1",
    "@nx/webpack": "17.0.3",
    "@nx/workspace": "17.0.3",
    "@parcel/watcher": "^2.3.0",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@swc/jest": "0.2.20",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.4.1",
    "cypress": "^13.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "17.0.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.5.2",
    "prisma-nestjs-graphql": "^19.2.0",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  }
}
